class Main {
    public static void main(String[] args) {
        int fact = 1;
        int num;
        for(int i =1;i<100;i++){
            num = i;
            for(int j = 1;j<i+1;j++){
                  fact = fact * j;
                  
            }
            if (fact > 362880){
              break;
            }
            System.out.println("Factorial of "+ num +"is"+fact);
            fact = 1;
        }
           }
}
